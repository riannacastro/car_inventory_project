<% if logged_in? %>
<h1> Hello, <%= current_user.username %>!</h1> 
<% end %>
<h2>Car Inventory:</h2>
<ul>
<% @cars.each do |c| %>
    <li><a href="/cars/<%=c.id%>"><%= c.year %> <%= c.make %> <%= c.model %></a></li>
    <% end %>
</ul><br>



<a href="/cars/new"><button>Create New Car</button></a>

<form method='POST' action='/logout'>
<input type="submit" value="Log Out">
</form>

      
